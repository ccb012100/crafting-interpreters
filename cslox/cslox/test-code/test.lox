/* scratch script for testing out functionality */
class A {
  method() {
    print "A method";
  }
}

class B < A {
  method() {
    print "B method";
  }

  test() {
    super.method();
  }
}

B().method(); // "B method"
B().test(); // "A method"

class C < B {}

C().test(); // "A method"

class Math {
  class square(n) {
    return n * n;
  }
}

print Math.square(3); // "9".

Math.pi = 3.141592653;
print Math.pi; // "3.14"

class Circle {
  init(radius) {
    this.radius = radius;
  }

  area {
    return 3.141592653 * this.radius * this.radius;
  }
}

var circle = Circle(4);
print circle.area; // "50.27"

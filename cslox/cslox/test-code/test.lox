/* scratch script for testing out functionality */

trait A {
  a() {
    print "a";
  }
}

trait B1 {
  b1() {
    print "b1";
  }
}

trait B2 {
  b2() {
    print "b2";
  }
}

trait B with B1, B2 {
  b() {
    this.b1();
    this.b2();
  }
}

class C with A, B {}

var c = C();
c.a(); // "a"
c.b();
// "b1"
// "b2"
